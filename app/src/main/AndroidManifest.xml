<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.picimaci.mobilsoft_lab3">

    <application
        android:name=".MobSoftApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.todo.TodoActivity" />
        <activity android:name=".ui.todolist.TodoListActivity" />
        <activity android:name=".ui.todolistlist.TodoListListActivity"></activity>
    </application>

</manifest>